<launch>
   
  <!-- load urdf, srdf -->
  <include file="$(find baxter_moveit_config)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>

  <!-- Display image on Baxter face -->
  <node name="face" pkg="head_control" type="xdisplay_image.py" 
	args="--file=$(find baxter_moveit_config)/media/hal.jpg" />

  <!-- mute Baxter's sonars -->
  <node name="baxter_mute_sonar" pkg="baxter_control" type="sonar_enable.py"
	respawn="false" output="screen" args="--enable=0"/>

  <!-- load Baxter's trajectory controller -->
  <node name="baxter_trajectory_controller" pkg="baxter_interface" type="trajectory_controller.py"
	respawn="false" output="screen"/>

  <!-- Load gripper controller -->
  <node name="baxter_gripper_server" pkg="baxter_gripper_server" type="gripper_action_server" 
	respawn="false" output="screen" />
  
  <!-- start move_group -->
  <include file="$(find baxter_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <!--arg name="debug" value="true"/-->
    <arg name="info" value="true"/>
  </include>

  <!-- start rviz -->
  <include file="$(find baxter_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
  </include>

  <!-- start warehouse -->
  <!-- include file="$(find baxter_moveit_config)/launch/default_warehouse_db.launch" /-->
</launch>
